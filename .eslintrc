{
  "env": {
    "node": true,
    "es6": true
  },
  "extends": [
    "eslint:recommended",
    "prettier"
  ],
  "plugins": [
      "prettier"
  ],
  "rules": {
    "prettier/prettier": 2,
    "no-unused-vars": 2,
    "no-param-reassign": 2,
    "dot-notation": 2,
    "eqeqeq": 2,
    "no-implied-eval": 2,
    "no-invalid-this": 2,
    "no-var": 2,
    "no-console": 0,
    "no-debugger": 0,
    "no-use-before-define": 2,
    "no-const-assign": 2,
    "spaced-comment": 2,
    "no-useless-constructor": 2,
    "prefer-const": 2,
    "max-depth": 2
  }
}